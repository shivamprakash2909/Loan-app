  "notes": {
    "customers": [
      "customer_name is optional but recommended",
      "account_number must be unique",
      "issue_date should be in YYYY-MM-DD format",
      "interest_rate is stored as DECIMAL",
      "tenure is in months (INTEGER)",
      "emi_due is stored as DECIMAL"
    ],
    "payments": [
      "payments reference customers by account_number in the request",
      "payment_amount cannot exceed emi_due",
      "payment_date is automatically set if not provided",
      "status is typically 'SUCCESS'",
      "Creating a payment automatically updates the customer's emi_due amount"
    ],
    "usage": [
      "Use this seed data to populate your database for testing",
      "Customers should be created before payments",
      "Payments reference customers by account_number",
      "The emi_due amounts in customers reflect the remaining balance after payments"
    ]
  }
