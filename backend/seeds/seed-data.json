        }
      },
      "GET /api/customers/:account_number": {
        "description": "Get customer by account number",
        "method": "GET",
        "endpoint": "/api/customers/ACC001",
        "response_example": {
          "id": 1,
          "customer_name": "Rajesh Kumar",
          "account_number": "ACC001",
          "issue_date": "2024-01-15T00:00:00.000Z",
          "interest_rate": "8.5",
          "tenure": 36,
