          "created_at": "2024-01-15T10:00:00.000Z"
        }
      },
      "POST /api/customers": {
        "description": "Create a new customer",
        "method": "POST",
        "endpoint": "/api/customers",
        "request_body": {
          "customer_name": "Rajesh Kumar",
          "account_number": "ACC001",
          "issue_date": "2024-01-15",
          "interest_rate": "8.5",
          "tenure": 36,
          "emi_due": "15000.00"
        },
        "response_example": {
          "id": 1,
          "customer_name": "Rajesh Kumar",
          "account_number": "ACC001",
          "issue_date": "2024-01-15T00:00:00.000Z",
          "interest_rate": "8.5",
          "tenure": 36,
          "emi_due": "15000.00",
          "created_at": "2024-01-15T10:00:00.000Z"
        }
      }
    },
    "payments": {
      "POST /api/payments": {
        "description": "Create a payment for a customer",
        "method": "POST",
        "endpoint": "/api/payments",
        "request_body": {
          "account_number": "ACC001",
          "payment_amount": "5000.00"
        },
        "response_example": {
          "message": "Payment processed successfully.",
          "payment": {
            "account_number": "ACC001",
            "paid_amount": "5000.00",
            "payment_date": "2024-02-15T10:30:00.000Z",
            "status": "SUCCESS"
          },
          "customer": {
            "account_number": "ACC001",
            "previous_emi_due": "15000.00",
            "new_emi_due": "10000.00"
          }
        }
      },
